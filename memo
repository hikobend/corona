CREATE TABLE `infection` (
  `id` int AUTO_INCREMENT PRIMARY KEY, 
  `date` date, 
  `name_jp` text, 
  `npatients` int
);

CREATE TABLE `events` (
  `id` int  AUTO_INCREMENT PRIMARY KEY,
  `title` text,
  `description` text,
  `begin` DATE,
  `end` DATE
);

CREATE TABLE `events` (
  `time` DATE
);

CREATE TABLE `decease` (
  `id` int AUTO_INCREMENT PRIMARY KEY, 
  `date` date, 
  `data_name` text, 
  `infected_num` int,
  `deceased_num` int
);

